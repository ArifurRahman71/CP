#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d,f,g,h;
    cin>>a>>b>>c>>d;
    vector<int>v;
    v.push_back(a);
    v.push_back(b);
    v.push_back(c);
    v.push_back(d);
    sort(v.begin(),v.end());
    a=v[3]-v[2];
    b=v[3]-v[1];
    c=v[3]-(a+b);
    cout<<a<<" "<<b<<" "<<c<<endl;
}
